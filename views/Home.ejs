<!DOCTYPE html>
<html lang="en">

<head>
    <%- include("./partials/Head") %>
</head>

<body>
    <%- include("./partials/Navbar") %>
        <div class="container my-5">
            <div class="row row-cols-4">


                <% blogs.forEach(element=> { %>
                    <div class="col my-3">

                        <div class="card col" style="width: 18rem;">
                            <img src="<%=element.coverImageURL %> " class="card-img-top" alt="...">
                            <div class="card-body">
                                <h5 class="card-title">
                                    <%= element.title %>
                                </h5>

                                <a href="blog/<%= element._id %>" class="btn btn-primary">View</a>
                            </div>
                        </div>
                    </div>
                    <% }) %>



            </div>
        </div>
        <%- include("./partials/Scripts") %>

</body>

</html>